<script type="text/javascript">
	jQuery(document).ready(function($)
	{
		$(".slideDownLink").click(function()
		{
			$(".slideDownBox").toggle();
		});

		$("input[name='page_title']").keyup(function()
		{
			var title = $(this).val();

			var path = (title.toLowerCase()).replace(/ /g, "-");

			var form = $(this).parents("form:first");

			form.find("input[name='page_path']").val(path);
			form.find(".path").html(path);
		});

		$(".path").click(function()
		{
			if(this.editing != true)
			{
				var self = this;
				var $self = $(this);

				self.editing = true;
				var path = $self.html();

				$self.html("<input class='edit-path' type='text' value='" + path + "' class='reset' style='display: inline' /> <a href='' class='save'>Save</a> <a href='' class='cancel'>Cancel</a>");
				$(".save", self).click(function()
				{
					var path = $(".edit-path", self).val();

					$self.html(path);

					$self.parents("form:first").find("input[name='page_path']").val(path);

					self.editing = false;

					return false;
				});

				$(".cancel", self).click(function()
				{
					$self.html(path);

					self.editing = false;

					return false;
				});
			}

			return false;
		});

		$(".preview").click(function()
		{
				tinyMCE.activeEditor.save();

			formData = $(this).parents("form:first").serialize();



			$.post("http://www.stokegames.com/admin/page/preview/", formData, function(data)
			{
				$("#page_preview")[0].contentWindow.document.body.innerHTML = data;
			});
		});

		$("#editorChooser").change(function()
		{
			window.location = "http://www.stokegames.com/admin/manage/pages/edit/32/?editor=" + $(this).val();
		});
	});
</script>

