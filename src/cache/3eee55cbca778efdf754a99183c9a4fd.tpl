<script type="text/javascript" src="http://www.stokegames.com/js/jquery.json.js"></script>
<script type="text/javascript">
	jQuery(document).ready(function($)
	{
		$("a.up").click(function()
		{
			var parentBox = $(this).parents(".box-1:first");
			var prevBox = parentBox.prev(".box-1");

			if(prevBox.length > 0)
			{
				parentBox.clone(true).insertBefore(prevBox);
				parentBox.remove();
			}
		
			return false;
		});

		$("a.down").click(function()
		{
			var parentBox = $(this).parents(".box-1:first");
			var nextBox = parentBox.next(".box-1");

			if(nextBox.length > 0)
			{
				parentBox.clone(true).insertAfter(nextBox);
				parentBox.remove();
			}

			return false;
		});

		$("div.box-1 h3").click(function()
		{
			var self = $(this);

			var box = self.parents(".box-1:first");
			var state = box.children(".state:first");
			var inside = state.children(".inside:first");

			state.toggleClass("opened");
		}).stopSelection();

		var enable = function()
		{
			$(this).parents("form:first").find("input[name='modules[" + $(this).attr("rel") + "][enabled]']").val("1");

			$(this).parents(".state:first").removeClass("disabled");

			$(this).html("Disable").attr("title", "Disable");

			$(this).unbind("click").bind("click", disable);

			return false;
		};

		var disable = function()
		{
			$(this).parents("form:first").find("input[name='modules[" + $(this).attr("rel") + "][enabled]']").val("0");

			$(this).parents(".state:first").addClass("disabled");

			$(this).html("Enable").attr("title", "Enable");

			$(this).unbind("click").bind("click", enable);

			return false;
		};

		$(".disable").bind("click", disable);

		$(".enable").bind("click", enable);

		$(".save").click(function()
		{
//"JSON=" + escape($.toJSON(modules)) + "&AJAX=1"
			if(confirm("Are you sure you want to this configuration?

Disabling or changing the order of database/core/cms modules may cause everything to stop working."))
			{
				$.post("http://www.stokegames.com/admin/manage/modules/", $(this).parents("form:first").serialize() + "&AJAX=1", function(data)
				{
					var $message = $("#message");

					$message.hide();

					if(data.status == 1)
						$message.addClass("success");
					else
						$message.addClass("error");

					$message.html(data.message).fadeIn(500, function()
					{
						setTimeout(function()
						{
							$message.fadeOut(500, function()
							{
								$message.html('');
							});
						}, 2000);
					});
				}, "json");
			}

			return false;
		});

	});
</script>						</div>
